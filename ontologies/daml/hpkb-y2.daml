   <rdf:Description rdf:ID="ISLAMIC-REFORM-MOVEMENT">
      <rdf:type rdf:resource="#DISSIDENT-GROUP"/>
      <rdf:type rdf:resource="#TERRORIST-GROUP"/>
      <HAS-SOURCE>
         
(listof
 &quot;U.S. Department of State, Saudi Arabia Report on Human Rights Practices for 1996&quot;
