   <rdf:Description rdf:ID="ELN-KIDNAPPS-TWO-FOREIGN-MEMBERS-OF-THE-ORGANIZATION-OF-AMERICAN-STATES">
      <rdf:type rdf:resource="#TERRORIST-ATTACK"/>
      <HAS-SOURCE>
         
(listof &quot;ICT - Terrorism and Counter-Terrorism&quot;
